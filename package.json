{
  "name": "enpii-wp-proj",
  "version": "1.0.0",
  "description": "Package for Enpii WordPress Proj",
  "scripts": {
    "build-theme": "webpack --config webpack-theme.config.js --mode production",
    "dev-theme": "webpack --config webpack-theme.config.js --watch --mode development",
    "i18n:make-pot-theme": "php -d xdebug.max_nesting_level=512 $(which wp) i18n make-pot --ignore-domain --exclude=\"node_modules/,languages/,assets/\" --headers='{\"Report-Msgid-Bugs-To\":\"https://github.com/enpiicom/wp-proj/issues/\", \"Language-Team\":\"Enpii Team <dev@enpii.com>\",\"Last-Translator\":\"Enpii Team <dev@enpii.com>\"}' public/wp-content/themes/appeara-alpha public/wp-content/themes/appeara-alpha/languages/appeara-alpha.pot",
    "i18n:update-po-theme": "php -d xdebug.max_nesting_level=512 $(which wp) i18n update-po public/wp-content/themes/appeara-alpha/languages/appeara-alpha.pot",
    "i18n:make-mo": "php -d xdebug.max_nesting_level=512 $(which wp) i18n make-mo",
    "build-plugin": "webpack --config webpack-plugin.config.js --mode production",
    "dev-plugin": "webpack --config webpack-plugin.config.js --watch --mode development"
  },
  "author": "dev@enpii.com",
  "license": "MIT",
  "keywords": [
    "enpii", "boilerplate", "wordpress", "laravel", "laravel wordpress", "enpii base"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.8",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "grunt": "^1.1.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-uncss": "^0.9.1",
    "purgecss": "^2.1.0",
    "purgecss-webpack-plugin": "^2.1.0",
    "purify-css": "^1.2.5",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.54.7",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack-build-notifier": "^2.0.0",
    "@babel/core": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "file-loader": "^6.2.0",
    "i": "^0.3.6",
    "mini-css-extract-plugin": "^2.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
