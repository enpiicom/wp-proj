{
  "name": "enpii-wp-proj",
  "version": "1.0.0",
  "description": "Package for Enpii WordPress Proj",
  "scripts": {
    "i18n:make-pot": "php -d xdebug.max_nesting_level=512 $(which wp) i18n make-pot --ignore-domain --exclude=\"node_modules/,languages/,assets/,vendor*/,tests/\" --headers='{\"Report-Msgid-Bugs-To\":\"https://github.com/enpiicom/wp-proj/issues/\", \"Language-Team\":\"Enpii Team <dev@enpii.com>\",\"Last-Translator\":\"Enpii Team <dev@enpii.com>\"}'",
    "i18n:update-po": "php -d xdebug.max_nesting_level=512 $(which wp) i18n update-po",
    "i18n:make-mo": "php -d xdebug.max_nesting_level=512 $(which wp) i18n make-mo",
    "codecept": "php -d xdebug.max_nesting_level=512 ./public/vendor/bin/codecept",
    "codecept:generate-unit": "php -d xdebug.max_nesting_level=512 ./public/vendor/bin/codecept generate:test unit",
    "codecept:unit": "php -d xdebug.max_nesting_level=512 -d xdebug.mode=coverage ./public/vendor/bin/codecept run unit",
    "codecept:unit-coverage": "php -d xdebug.max_nesting_level=512 -d xdebug.mode=coverage ./public/vendor/bin/codecept run --coverage --coverage-xml --coverage-html unit",
    "phpunit": "php -d xdebug.max_nesting_level=512 ./public/vendor/bin/phpunit",
    "phpunit:coverage": "php -d xdebug.max_nesting_level=512 -d xdebug.mode=coverage ./public/vendor/bin/phpunit --coverage-text -vvv",
    "phpunit:coverage-html": "php -d xdebug.max_nesting_level=512 -d xdebug.mode=coverage ./public/vendor/bin/phpunit --coverage-html",
    "phpunit:coverage-single": "php -d xdebug.max_nesting_level=512 -d xdebug.mode=coverage ./public/vendor/bin/phpunit --coverage-text --no-configuration --colors --bootstrap=tests/codeception-bootstrap.php",
    "phpcbf": "php -d xdebug.max_nesting_level=512 ./public/vendor/bin/phpcbf",
    "phpcs": "php -d xdebug.max_nesting_level=512 ./public/vendor/bin/phpcs",
    "build-plugin": "webpack --config webpack-plugin.config.js --mode=production",
    "dev-plugin": "webpack --config webpack-plugin.config.js --watch --mode=development",
    "build-theme": "webpack --config webpack-theme.config.js --mode=production",
    "dev-theme": "webpack --config webpack-theme.config.js --watch --mode=development"
  },
  "author": "dev@enpii.com",
  "license": "MIT",
  "keywords": [
    "enpii",
    "boilerplate",
    "wordpress",
    "laravel",
    "laravel wordpress",
    "enpii base"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^3.6.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv": "^16.4.5",
    "dotenv-webpack": "^8.1.0",
    "file-loader": "^6.2.0",
    "i": "^0.3.7",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.77.0",
    "sass-loader": "^14.2.1",
    "source-map-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-build-notifier": "^2.3.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {}
}
